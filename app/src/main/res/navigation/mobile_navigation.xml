<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_feeds"
        android:name="com.example.androidkotlin.ui.home.HomeFragment"
        android:label="@string/title_feeds"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_find_Friends"
        android:name="com.example.androidkotlin.ui.dashboard.DashboardFragment"
        android:label="@string/title_find_friends"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.example.androidkotlin.ui.notifications.NotificationsFragment"
        android:label="@string/title_message"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.androidkotlin.ui.notifications.NotificationsFragment"
        android:label="@string/title_message"
        tools:layout="@layout/fragment_notifications" />
</navigation>